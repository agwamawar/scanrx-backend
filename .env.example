# EMDEX API Configuration
# Copy this file to .env and fill in your credentials

# Set to 'true' to use mock EMDEX data for development/testing
# Set to 'false' (or remove) when real EMDEX credentials are ready
USE_MOCK_EMDEX=true

# Real EMDEX API credentials (only needed when USE_MOCK_EMDEX=false)
EMDEX_API_URL=https://sandbox.emdexapi.com
EMDEX_EMAIL=your_emdex_email
EMDEX_PASSWORD=your_emdex_password
